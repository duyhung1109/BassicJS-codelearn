function commonCharacterCount(s1, s2){
    arr1 = s1.split('');
    arr2 = s2.split('');

      while( arr2.indexOf(arr1[0]) !== -1 ){
        arr2.splice(arr2.indexOf(arr1[0]), 1);
        arr1.splice(0, 1);
         if( arr1.length == 0 && arr2.length == 0 ){ return true; }
      }

    return false;
}
