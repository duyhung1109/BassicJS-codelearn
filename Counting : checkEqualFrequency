function checkEqualFrequency(inputArray){
  let differentArr = inputArray;
  let i = 0;
  console.log(differentArr.indexOf(5));
  differentArr.forEach( function(value,index){ 
    
    if( differentArr.indexOf(differentArr[index]) != -1 ){
      console.log('value', differentArr[index]);
      console.log('index', differentArr.indexOf(differentArr[index]));
      differentArr.splice( differentArr.lastIndexOf(differentArr[index]), 1 );
    }
  });
  
  console.log(inputArray.length);
  console.log(differentArr);

  if( inputArray.length % differentArr.length == 0 ){
    return true;
  }
  return false;
}

checkEqualFrequency([1, 5, 2, 1, 2]);
